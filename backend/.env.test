# Test Environment
NODE_ENV=test
PORT=5001

# MongoDB (test database - separate from dev!)
MONGODB_URI=mongodb://localhost:27017/diabetic-marketplace-test

# JWT Configuration
JWT_SECRET=test-jwt-secret-for-testing-purposes-only
JWT_EXPIRE=1h

# Frontend URL
FRONTEND_URL=http://localhost:3000

# Rate Limiting (disabled for tests)
RATE_LIMIT_WINDOW_MS=1000
RATE_LIMIT_MAX=1000
AUTH_RATE_LIMIT_MAX=100

# Logging (silent for tests)
LOG_LEVEL=error

# File Upload
MAX_FILE_SIZE=1048576
MAX_FILES=2
